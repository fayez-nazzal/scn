{
  "name": "scn",
  "version": "1.0.19",
  "description": "Simple classnames utility function that is tiny and fast (75 bytes)",
  "main": "lib/scn.js",
  "files": [
    "lib/**/*"
  ],
  "types": "lib/scn.d.ts",
  "scripts": {
    "build": "tsc && google-closure-compiler --js=lib/scn.js --js_output_file=lib/scn.min.js && mv lib/scn.min.js lib/scn.js",
    "test": "jest --config jestconfig.json"
  },
  "keywords": [
    "classes",
    "classname",
    "className",
    "classnames",
    "simple classes",
    "simple classnames",
    "class",
    "class names",
    "class names",
    "tiny",
    "fast",
    "small",
    "smaller",
    "organize",
    "organize class",
    "organize classnames",
    "clsx",
    "classnames"
  ],
  "author": "Fayez Nazzal",
  "license": "MIT",
  "repository": "https://github.com/fayez-nazzal/scn",
  "bugs": "https://github.com/fayez-nazzal/scn/issues",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "google-closure-compiler": "^20230103.0.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
